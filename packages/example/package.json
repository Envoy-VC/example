        "dotenv": "^16.3.1",
        "ethers": "v5",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "vitest": "^1.0.4",
        "warp-contracts": "^1.4.26",
        "warp-contracts-plugin-deploy": "^1.0.12"
